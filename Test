						local BindToggle = {
							State = options.Enabled;
							Hover = false;

							BindHover = false;
							WaitingAbutton = false;
							HasBind = false;
							BindButton = "";

							Name = options.Title;
							Type = "BindToggle";
						}

						--/// DRAWING
						do
							-- StarterGui.EraGui.MainWindow.Background.ContentHolder.AnimationHolder.ScrollingFrame.Category.BindToggle
