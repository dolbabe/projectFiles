							BindToggle["90"]["AnchorPoint"] = Vector2.new(1, 0.5)
							BindToggle["90"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
							BindToggle["90"]["Name"] = [[BindFrame]];
